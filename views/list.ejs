<%- include('header') %>
  <div class="box">
    <% for( let i=0; i < newListItems.length; i++ ) { %>
      <div class="item">
        <input type="checkbox">
        <p>
          <%= newListItems[i] %>
        </p>
      </div>
      <% } %>
        <form class="form" action="/" method="post">
          <input class="new-item" type="text" name="newItem" placeholder="New Item" autocomplete="off">
          <button type="submit" name="list" value=<%=ListTitle %>>+</button>
        </form>
  </div>

  <%- include('footer') %>